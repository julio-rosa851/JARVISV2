{
  "name": "jarvis-v2",
  "version": "2.0.0",
  "description": "Sistema JARVIS V2 - Assistente Empresarial com IA",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "node start.js"
  },
  "keywords": [
    "jarvis",
    "ai",
    "assistant",
    "enterprise",
    "fastapi",
    "react",
    "agents"
  ],
  "author": "JARVIS Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}